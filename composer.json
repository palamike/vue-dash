{
    "name": "palamike/vue-dash",
    "description": "Laravel and Vue CRUD Admin Panel Generator",
    "type": "package",
    "license": "PLM",
    "authors": [
        {
            "name": "palamike",
            "email": "palagorn@gmail.com"
        }
    ],
    "minimum-stability": "dev",

    "require": {
        "php": "^7.1.3",
        "laravel/framework": "5.6.*",
        "spatie/laravel-permission": "^2.9.0"
    },

    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Palamike\\VueDash\\": "src/VueDash"
        },
        "files": [
            "src/VueDash/Functions/blade.php",
            "src/VueDash/Functions/utilities.php"
        ]
    },

    "extra": {
        "laravel": {
            "providers": [
                "Palamike\\VueDash\\Providers\\VueDashServiceProvider",
                "Palamike\\VueDash\\Providers\\SettingServiceProvider"
            ]
        }
    }
}
